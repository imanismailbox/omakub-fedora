# History control (Zsh)
setopt APPEND_HISTORY
setopt HIST_IGNORE_DUPS
setopt HIST_IGNORE_SPACE
HISTSIZE=32768
SAVEHIST=${HISTSIZE}

# Autocompletion (Zsh)
autoload -Uz compinit
compinit

# Set complete path
export PATH="./bin:$HOME/.local/bin:$HOME/.local/share/omakub/bin:$PATH"
export OMAKUB_PATH="/home/$USER/.local/share/omakub"
